# Prints predefined GCC macro

set -eu

CC=gcc
if test $# -gt 0; then
  if which "$1" >/dev/null 2>&1; then
    CC="$1"
    shift
  fi
fi

TMP=$(mktemp --suffix=.h)
trap "rm -f $TMP" EXIT INT TERM

$CC "$@" -E -dM $TMP
