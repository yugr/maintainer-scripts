  line-too-long,
  bad-indentation,
  invalid-name,
  too-many-locals,
  too-many-branches,
  too-many-statements,
  too-many-arguments,
  too-few-public-methods,
  too-many-instance-attributes,
  too-many-lines,
  missing-docstring,
  misplaced-comparison-constant,
  global-statement
